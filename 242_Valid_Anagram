class Solution {
    public boolean isAnagram(String s, String t) {
        char[] s2 = s.toCharArray();
        char[] t2 = t.toCharArray();
        Arrays.sort(s2);
        Arrays.sort(t2);
        Arrays.toString(s2);
        Arrays.toString(t2);
        String s3 = new String(s2);
        String t3 = new String(t2);
        System.out.println(s3);
        System.out.println(t3);
        if (s3.equals(t3)) {
            return true;
        }
        else {
            return false;
        }
    }
}
